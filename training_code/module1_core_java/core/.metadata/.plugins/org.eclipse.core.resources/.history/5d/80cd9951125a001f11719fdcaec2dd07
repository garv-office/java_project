package com.day3.session3.interface_example;
class StackImpUsingArray {
	private int x[];
	private final int SIZE;
	private int top;
	
	public StackImpUsingArray() {
		x=new int[10];
		SIZE=10;
		top=-1;	
	}
	
	
	public void pushRavi(int element){
		if(top>=SIZE)
			return ;
		else
			x[top++]=element;
	}
	
	
	public int popRavi(){
		if(top==-1)
			return -99;
		else
			return x[top--];
	}
}