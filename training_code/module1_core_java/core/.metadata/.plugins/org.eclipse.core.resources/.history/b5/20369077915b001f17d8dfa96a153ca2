package com.day4.session1.b.pending_topics_inner;
//how to make lambda from ann inner class
@FunctionalInterface
interface Cal{
	int add(int a, int b) ;
}
public class E_Lambda {
	
	public static void main(String[] args) {
		Cal cal=new Cal() {
			
			@Override
			public int add(int a, int b) {
				return a+b;
			}
		};
	}

}
