package com.day4.session3;
//hotel 
class Juice{
	
}
class Glass{
	private Juice juice;

	public Juice getJuice() {
		return juice;
	}

	public void setJuice(Juice juice) {
		this.juice = juice;
	}
	
	
}
public class E_GenericsAndBL {
	
	public static void main(String[] args) {
		Glass glass=new Glass();
		glass.setJuice(new Juice());
		Juice juice=glass.getJuice();
		
	}

}
