package com.day4.session3;

import java.util.LinkedList;
import java.util.List;

public class A_Intro_Generics {

	public static void main(String[] args) {
		//Why to use generics?
		//Generics is type essared?
		List<String> list = new LinkedList<>();
		list.add("foo");
		list.add("bar");
		list.add("foo");
		list.add("apple");
		list.add("umesh");
	}
}
