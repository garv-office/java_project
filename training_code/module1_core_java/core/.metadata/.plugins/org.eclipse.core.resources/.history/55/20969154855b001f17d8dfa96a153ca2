package com.day4.session1.pending_topics;

class Outer {
	private int i = 10;

	class Inner {
		private int i = 20;

		void foo() {
			System.out.println("foo method ..."+this.i);
		}
	}

	 void bar() {
		Inner inner = new Inner();
		inner.foo();
	}
}

public class B_InnerClasses {

	public static void main(String[] args) {
			Outer outer=new Outer();
			outer.bar();
	}

}
